{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "allowJs": false,
    "esModuleInterop": true,
    "incremental": true,
    "isolatedModules": true,
    "lib": [
      "ES2023"
    ],
    "module": "node16",
    "moduleResolution": "node16",
    "resolveJsonModule": true,
    "strict": true,
    "skipLibCheck": true,
    "target": "ES2022"
  }
}
